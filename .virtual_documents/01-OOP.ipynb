class SuperKahraman():
    def __init__(self,isim,yas,meslek):
        self.isim = isim
        self.yas = yas
        self.meslek = meslek
        
            
    def ornekMethod(self):
        print(f"benim mesleğim: {self.meslek} ")


superman = SuperKahraman("Clark Kent",30,"Gazeteci")


superman.isim


superman.ornekMethod()


